package Basics;

public class Cities {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	
		String[] cities = {"Jaypur","Hyderabad","Delhi","Banglore","Bhubaneswar"};
        do {
        	int i=0;
        	System.out.println(cities[i]);
        	i++;
        }while(i<5);
	}

}
